  Global("CDMarkethTeleporting","GLOBAL",1)
  Global("CDMarkHere","AR8001",0)
  !Global("Marketh_Ring","GLOBAL",6)
THEN
  RESPONSE #100
    SetGlobal("CDMarkHere","AR8001",1)
